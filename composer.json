{
	"name": "tas2580/extlist",
	"type": "phpbb-extension",
	"description": "Add a new page that display all installed extensions to the user.",
	"homepage": "https://tas2580.net/downloads/phpbb-extension-list/",
	"version": "0.1.2",
	"keywords": ["phpbb", "extension"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "tas2580",
			"homepage": "https://tas2580.net",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "tas2580 Extension list",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.3-RC1,<3.2.*@dev"
		},
		"version-check": {
			"host": "tas2580.net",
			"directory": "/modules/downloads/version",
			"filename": "download9.json"
		}
	}
}
